{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-multiple-empty-lines": "off",
    "eol-last": "off",
    "linebreak-style": "off",
    "import/no-extraneous-dependencies": "off",
    "no-console": "off",
    "operator-linebreak": "off",
    "comma-dangle": "off",
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off"
  }
}
